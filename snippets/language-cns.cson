'.source.cns':
  'Func':
    'prefix': '=>'
    'body': '=>\n\t${1:# expressions...}\n'
  'When':
    'prefix': 'when'
    'body': 'when\n\t${1:# condition -> ...}\n'
  'Match':
    'prefix': 'match'
    'body': 'match\n\t${1:# pattern -> ...}\n'
  'Caseof':
    'prefix': 'caseof'
    'body': 'caseof ${1:value}\n  ${2:condition} -> ${0:# body...}\n'
  'Try â€¦ Catch':
    'prefix': 'try'
    'body': 'try\n\t${1:# body...}\ncatch ${2:err}\n\t${0:# body...}\n'
'.string.quoted.double.cns:not(.string .source), .string.quoted.double.heredoc.cns:not(.string .source)':
  'Interpolated Code':
    'prefix': '$'
    'body': '${$1}$2'
